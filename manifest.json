{
  "manifest_version": 2,

  "name": "TeamCity Alerts",
  "description": "Show alerts for each build.",
  "version": "1.0",
  "background": {
    "scripts": ["teamCityAlert.js","app.js"],
    "persistent": true
  },
  "options_page": "options.html",
  "permissions": [
    "http://*/","notifications"
  ]
}
